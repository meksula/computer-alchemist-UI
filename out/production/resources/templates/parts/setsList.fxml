<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="432.0" prefWidth="313.0" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.computeralchemist.desktop.gui.controller.interaction.manager.SetListButtonManager">
    <children>
        <Label layoutX="94.0" layoutY="6.0" styleClass="txt" text="Request properties" />
        <Label layoutX="14.0" layoutY="63.0" styleClass="txt" text="Choose set type:" />
        <Button fx:id="executeButton" layoutX="94.0" layoutY="352.0" mnemonicParsing="false" onAction="#executeRequest" text="Execute request" />
        <MenuButton fx:id="componentMenu" layoutX="14.0" layoutY="79.0" mnemonicParsing="false" text="Components">
            <items>
                <MenuItem fx:id="gamingSet" mnemonicParsing="false" onAction="#setComponentType" text="Gaming computer set" />
                <MenuItem fx:id="familySet" mnemonicParsing="false" onAction="#setComponentType" text="Family computer set" />
                <MenuItem fx:id="workSet" mnemonicParsing="false" onAction="#setComponentType" text="Work computer set" />
            </items>
        </MenuButton>
    </children>
</Pane>